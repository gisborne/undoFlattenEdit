#tag WindowCode
	#tag Method, Flags = &h0
		Sub Display(q As String)
		  Question.Text = q
		  #If TargetCarbon
		    ShowModalWithin Window(1)
		  #Else
		    ShowModal
		  #EndIf
		End Sub
	#tag EndMethod


